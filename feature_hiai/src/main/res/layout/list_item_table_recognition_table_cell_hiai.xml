<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <LinearLayout
        android:id="@+id/ll_table_recognition_table_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="10">

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/tv_table_recognition_cell_count"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/TextAppearance.ExploreHMS.Body2"
                android:layout_gravity="center"
                android:gravity="center">
            </com.google.android.material.textview.MaterialTextView>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_marginStart="8dp">

            <LinearLayout
                android:id="@+id/ll_table_recognition_table_cell_word"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_weight="1"
                android:layout_marginTop="8dp"
                android:orientation="horizontal">

                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/TextAppearance.ExploreHMS.Body2"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="@string/txt_table_recognition_cell_word"
                    android:layout_weight="2">
                </com.google.android.material.textview.MaterialTextView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_table_recognition_table_cell_word"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/TextAppearance.ExploreHMS.Body2"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="1">
                </com.google.android.material.textview.MaterialTextView>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_table_recognition_table_start_col"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/ll_table_recognition_table_cell_word"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/TextAppearance.ExploreHMS.Body2"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="@string/txt_table_recognition_cell_start_col"
                    android:layout_weight="2">
                </com.google.android.material.textview.MaterialTextView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_table_recognition_table_start_col"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/TextAppearance.ExploreHMS.Body2"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="1">
                </com.google.android.material.textview.MaterialTextView>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_table_recognition_table_start_row"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/ll_table_recognition_table_start_col"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/TextAppearance.ExploreHMS.Body2"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="@string/txt_table_recognition_cell_start_row"
                    android:layout_weight="2">
                </com.google.android.material.textview.MaterialTextView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_table_recognition_table_start_row"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/TextAppearance.ExploreHMS.Body2"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="1">
                </com.google.android.material.textview.MaterialTextView>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_table_recognition_table_end_col"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/ll_table_recognition_table_start_row"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/TextAppearance.ExploreHMS.Body2"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="@string/txt_table_recognition_cell_end_column"
                    android:layout_weight="2">
                </com.google.android.material.textview.MaterialTextView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_table_recognition_table_end_col"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/TextAppearance.ExploreHMS.Body2"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="1">
                </com.google.android.material.textview.MaterialTextView>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_table_recognition_table_end_row"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/ll_table_recognition_table_end_col"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/TextAppearance.ExploreHMS.Body2"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="@string/txt_table_recognition_cell_end_row"
                    android:layout_weight="2">
                </com.google.android.material.textview.MaterialTextView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_table_recognition_table_end_row"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/TextAppearance.ExploreHMS.Body2"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="1">
                </com.google.android.material.textview.MaterialTextView>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray1HiAi"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toBottomOf="@+id/ll_table_recognition_table_result">
    </View>


</androidx.constraintlayout.widget.ConstraintLayout>