            android:layout_margin="16dp"
            android:adjustViewBounds="true"
            android:contentDescription="@string/phased_release_overview_title"
            android:src="@drawable/phased_release_overview4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/desc4" />

        <TextView
            android:id="@+id/desc5"
            style="@style/TextAppearance.ExploreHMS.Body1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            android:text="@string/phased_release_overview_desc5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/image4" />

        <ImageView
            android:id="@+id/image5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
