<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.GameServicePlayerStatisticsFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginBottom="@dimen/h16"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/tvHeaderPlayerStatistics_gameservices"
                style="@style/TextAppearance.ExploreHMS.Headline5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/h16"
                android:layout_marginTop="@dimen/h16"
                android:layout_marginEnd="@dimen/h16"
                android:text="@string/service_introduction"
                app:layout_constraintTop_toTopOf="parent" />

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/tvInfoPlayerStatistics_gameservices"
                style="@style/TextAppearance.ExploreHMS.Body1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/h16"
                android:layout_marginTop="@dimen/h16"
                android:layout_marginEnd="@dimen/h16"
                android:text="@string/info_playerstatistics_gameservices"
                app:layout_constraintTop_toBottomOf="@+id/tvHeaderPlayerStatistics_gameservices" />


            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/h16"
                android:layout_marginTop="@dimen/h16"
                android:layout_marginEnd="@dimen/h16"
                android:background="@drawable/bg_gray_result"
                app:layout_constraintTop_toBottomOf="@+id/tvInfoPlayerStatistics_gameservices">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvHeadPlayerStatistics_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Headline6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    android:gravity="center_horizontal"
                    android:text="@string/player_statistics"
                    app:layout_constraintTop_toTopOf="parent" />

                <View
                    android:id="@+id/separator1Playerstatistics_gameservices"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/h16"
                    android:background="@color/colorGray1"
                    app:layout_constraintTop_toBottomOf="@+id/tvHeadPlayerStatistics_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvAvarageSessionHeader_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    android:text="@string/average_session_length"
                    app:layout_constraintTop_toBottomOf="@+id/separator1Playerstatistics_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvAverageSessionInfo_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    app:layout_constraintTop_toBottomOf="@+id/tvAvarageSessionHeader_gameservices" />

                <View
                    android:id="@+id/separator2Playerstatistics_gameservices"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/h16"
                    android:background="@color/colorGray1"
                    app:layout_constraintTop_toBottomOf="@+id/tvAverageSessionInfo_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvDaySinceLastHeader_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    android:text="@string/day_since_last_played"
                    app:layout_constraintTop_toBottomOf="@+id/separator2Playerstatistics_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvDaySinceLastInfo_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    app:layout_constraintTop_toBottomOf="@+id/tvDaySinceLastHeader_gameservices" />

                <View
                    android:id="@+id/separator3Playerstatistics_gameservices"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/h16"
                    android:background="@color/colorGray1"
                    app:layout_constraintTop_toBottomOf="@+id/tvDaySinceLastInfo_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvNumberOfPurchasesHeader_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    android:text="@string/number_of_purchases"
                    app:layout_constraintTop_toBottomOf="@+id/separator3Playerstatistics_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvNumberOfPurchasesInfo_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    app:layout_constraintTop_toBottomOf="@+id/tvNumberOfPurchasesHeader_gameservices" />

                <View
                    android:id="@+id/separator4Playerstatistics_gameservices"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/h16"
                    android:background="@color/colorGray1"
                    app:layout_constraintTop_toBottomOf="@+id/tvNumberOfPurchasesInfo_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvNumberOfSessionHeader_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    android:text="@string/number_of_sessions"
                    app:layout_constraintTop_toBottomOf="@+id/separator4Playerstatistics_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvNumberOfSessionInfo_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    app:layout_constraintTop_toBottomOf="@+id/tvNumberOfSessionHeader_gameservices" />

                <View
                    android:id="@+id/separator5Playerstatistics_gameservices"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/h16"
                    android:background="@color/colorGray1"
                    app:layout_constraintTop_toBottomOf="@+id/tvNumberOfSessionInfo_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvTotalPurchasesHeader_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    android:text="@string/total_purchases_amount_range"
                    app:layout_constraintTop_toBottomOf="@+id/separator5Playerstatistics_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvTotalPurchasesInfo_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    app:layout_constraintTop_toBottomOf="@+id/tvTotalPurchasesHeader_gameservices" />

                <View
                    android:id="@+id/separator6Playerstatistics_gameservices"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/h16"
                    android:background="@color/colorGray1"
                    app:layout_constraintTop_toBottomOf="@+id/tvTotalPurchasesInfo_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvDescribeContenstHeader_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    android:text="@string/describe_contents"
                    app:layout_constraintTop_toBottomOf="@+id/separator6Playerstatistics_gameservices" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvDescribeContentsInfo_gameservices"
                    style="@style/TextAppearance.ExploreHMS.Body1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/h16"
                    android:layout_marginTop="@dimen/h16"
                    android:layout_marginEnd="@dimen/h16"
                    android:layout_marginBottom="@dimen/h16"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvDescribeContenstHeader_gameservices" />


            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>