<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.GameServiceGameTurboEngineFragment">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tvHeaderGTE_computergraphics"
        style="@style/TextAppearance.ExploreHMS.Headline5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/h16"
        android:layout_marginTop="@dimen/h16"
        android:layout_marginEnd="@dimen/h16"
        android:text="@string/service_introduction"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tvInfoGTE_computergraphics"
        style="@style/TextAppearance.ExploreHMS.Body1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/h16"
        android:layout_marginTop="@dimen/h16"
        android:layout_marginEnd="@dimen/h16"
        android:text="@string/info_gte_computergraphics"
        app:layout_constraintTop_toBottomOf="@+id/tvHeaderGTE_computergraphics" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/h16"
        android:layout_marginTop="@dimen/h16"
        android:layout_marginEnd="@dimen/h16"
        android:background="@drawable/bg_gray_result"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvInfoGTE_computergraphics">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/tmpGTE_gameservices"
            style="@style/TextAppearance.ExploreHMS.Headline6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/h16"
            android:layout_marginTop="@dimen/h16"
            android:layout_marginEnd="@dimen/h16"
            android:gravity="center_horizontal"
            android:text="@string/result"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/seperator1GTE_gameservices"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/h16"
            android:background="@color/colorGray1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tmpGTE_gameservices" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/tvPhoneInfoHeader_gameservices"
            style="@style/TextAppearance.ExploreHMS.Body1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/h16"
            android:layout_marginTop="@dimen/h16"
            android:layout_marginEnd="@dimen/h16"
            android:text="@string/phone_info"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/seperator1GTE_gameservices" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/tvPhoneInfoGTE_computergraphics"
            style="@style/TextAppearance.ExploreHMS.Body1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/h16"
            android:layout_marginTop="@dimen/h16"
            android:layout_marginEnd="@dimen/h16"
            app:layout_constraintTop_toBottomOf="@+id/tvPhoneInfoHeader_gameservices" />

        <View
            android:id="@+id/seperator2GTE_gameservices"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/h16"
            android:background="@color/colorGray1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvPhoneInfoGTE_computergraphics" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/tvGameRegisteredHeaderGTE_gameservices"
            style="@style/TextAppearance.ExploreHMS.Body1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/h16"
            android:layout_marginTop="@dimen/h16"
            android:layout_marginEnd="@dimen/h16"
            android:text="@string/game_register"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/seperator2GTE_gameservices" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/tvGameRegisteredGTE_gameservices"
            style="@style/TextAppearance.ExploreHMS.Body1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/h16"
            android:layout_marginTop="@dimen/h16"
            android:layout_marginEnd="@dimen/h16"
            android:layout_marginBottom="@dimen/h16"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvGameRegisteredHeaderGTE_gameservices" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>